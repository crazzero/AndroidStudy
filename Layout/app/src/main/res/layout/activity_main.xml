<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/moveToFrame"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/move.to.frame" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/moveToConstraint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/move.to.constraint" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/moveToRelative"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/move.to.relative" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/moveToLinear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/move.to.linear" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/moveToFlex"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/move.to.flex" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/moveToCoordinator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/move.to.coordinator" />

</LinearLayout>
